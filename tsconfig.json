{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "allowJs": false,
    "jsx": "react-native",
    "strict": true,
    "moduleResolution": "node",
    "module": "commonjs",
    "target": "es6",
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": ["**/*.ts", "**/*.tsx", ".expo/types/**/*.ts", "expo-env.d.ts"]
}
